{
	"extends": [
		"react-app",
		"react-app/jest"
	],
	"rules": {
		"no-console": "warn",
		"prefer-const": "warn",
		"indent": ["warn", 2],
		"semi": ["warn", "always"],
		"import/order": ["warn", {
			"groups": ["builtin", "external", "internal", "parent", "sibling", "index", "object", "type"],
			"newlines-between": "always-and-inside-groups"
		}]
	}
}